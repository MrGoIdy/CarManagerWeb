@model List<string>

@if (Model.Count > 0)
{
    <h3>@ViewBag.Brand models:</h3>
        <table class="table">
            <thead>
                <tr>
                    <th>
                        Model
                    </th>
                </tr>
            </thead>
            @foreach (var carModel in Model)
            {
                <tr>
                    <td>
                        @carModel
                    </td>
                </tr>
            }
        </table>
}
else
{
    <h4 style="color:slategray">There are no Models found in the DataBase for brand @ViewBag.Brand</h4>
}

